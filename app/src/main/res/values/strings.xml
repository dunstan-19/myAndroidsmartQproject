<resources>
    <string name="app_name">SmartQ</string>
    <string name="open_nav"> Open Hello</string>
    <string name="close_nav"> Close Hello</string>
    <!-- TODO: Remove or change this placeholder text -->
    <string name="hello_blank_fragment">Hello blank fragment</string>
    <string name="Cursive">Cursive</string>

    <!-- Custom toolbar style -->
    <style name="CustomToolbar" parent="Widget.AppCompat.Toolbar">
        <item name="background">?attr/colorPrimary</item>
        <item name="popupTheme">@style/CustomPopupTheme</item>
    </style>

    <!-- Custom popup theme for menu items -->
    <style name="CustomPopupTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:itemBackground">@color/white</item>
    </style>

    <!-- Custom menu item text color -->
    <style name="CustomMenuItemText" parent="TextAppearance.AppCompat.Widget.ActionBar.Menu">
        <item name="android:textColor">@color/black</item>
    </style>


</resources>